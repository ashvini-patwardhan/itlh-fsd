    <style>
        /* CSS Comments */

        p {
            color: #333;
            /*            (red / rgb(255, 0, 0) / #F00 / #FF0000) */
            text-align: center;
            /*            (left / right / center/ justify) */
            text-decoration: underline;
            /*            (overline/ line-through / underline) */
            text-transform: uppercase;
            /*            (uppercase / lowercase / capitalize) */
            font-size: 15px;
            font-family: Times New Roman;
            letter-spacing: 0.5px;
            word-spacing: 5px;
            line-height: 18px;
        }

        body {
            background-color: #f5f894;
        }

        body {
            background-image: url('img2.jpg');
        }

        body {
            background-repeat: none;
        }

        body {
            background-position: left top;
        }

        div {
            height: 100px;
        }

        div {
            width: 100px;
        }

        div {
            min-height: 100px;
        }

        div {
            max-height: 150px;
        }

        div {
            min-width: 60px;
        }

        div {
            max-width: 250px;
        }

        div {
            margin-top: 30px;
            margin-right: 30px;
            margin-bottom: 30px;
            margin-left: 30px;
        }

        #div1 {
            position: static;
            color: aliceblue;
        }

        #div2 {
            position: relative;
            left/right: 50px;
            color: aliceblue;
        }

        #div3 {
            position: fixed;
            left/right: 50px;
            top/bottom: 50px;
            color: aliceblue;
        }

        #div4 {
            position: inherit;
            left/right: 50px;
            top/bottom: 50px;
            color: aliceblue;
        }

        #div4a {
            position: absolute;
            left/right: 5px;
            top/bottom: 5px;
            color: aliceblue;
        }

        #div1 {
            float: left;
        }

        #div2 {
            clear: right;
        }

        div {
            background-color: #901111;
            width: 300px;
            height: 100px;
        }

        #div1 {
            overflow: visible;
        }

        #div2 {
            overflow: hidden;
        }

        #div2 {
            overflow: scroll;
        }

        #div2 {
            overflow: auto;
        }

        #div1 {
            display: block;
        }

        #div2 {
            clear: block;
        }

        #div3 {
            clear: inline-block;
        }

    </style>


